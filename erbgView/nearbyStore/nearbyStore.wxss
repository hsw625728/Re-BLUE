/* erbgView/nearbyStore/nearbyStore.wxss */

.m-menu {
  display: flex;
  height: 80rpx;
  width: 750rpx;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: space-between;
  background-color: #fff;
}

.m-menu .item {
  flex: 1;
  display: block;
  padding: 20rpx 0;
}

.m-menu image {
  display: block;
  width: 58rpx;
  height: 58rpx;
  margin: 0 auto;
  margin-bottom: 12rpx;
}

.m-menu text {
  display: block;
  font-family:Times;
  font-size: 24rpx;
  text-align: center;
  margin: 0 auto;
  line-height: 1;
  color: #333;
}

.addressList{
}

.addressList .cell {
  width: 100%;
  height: 180rpx;
  background: #ffffff;
  border-bottom: 0.1px solid rgba(0, 0, 0, 0.1);
  border-radius: 5rpx;
  -webkit-column-break-inside: avoid;
  overflow: hidden;
}
.addressList .cell .a {
}
.addressList .cell .a .img{
  float:left;
  width: 180rpx;
  height: 180rpx;
  border-radius: 5rpx;
}

.addressList .cell .a .info{
  float: right;
  width: 100%;
  margin-left:-180px;
  padding-left: 180rpx;
  height: 180rpx;
  -webkit-column-break-inside: avoid;
}


.addressList .cell .a .name{
  padding: 10rpx 10rpx;
  display:block;
  color: #000000;
  font-size: 26rpx;
}

.addressList .cell .a .address {
  padding: 10rpx 10rpx;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: space-between;
}

.addressList .cell .a .address .addressText{
  display:block;
  padding-top: 13%;
  font-size: 26rpx;
}


